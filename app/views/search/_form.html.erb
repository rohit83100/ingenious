<%= form_for(@q, url: search_path, method: :get) do |f| %>
    <%= f.search_field :email_cont, placeholder: "Search...", style:"width:250px; height:40px; border-radius:10px; padding-left:10px;" %>
    <%= f.submit "Search!", style:"width:80px; height:40px; background:blue; color:white; border-radius:15px; " %>
<% end %>