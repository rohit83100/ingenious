<div class="cont-<%= message.user.id %>">
    <div class="message-box msg-<%= message.user.id %>" style="margin-left:600px;">
        <% unless message.room.is_private %>
            <% end %>
                <h6 class="name">
                    <%= message.user.email %>
                </h6>
                <%= message.body %>
    </div>
</div>